{
    "name": "YunoDash",
    "id": "yunodash",
    "description": {
        "en": "Yunohost applications dashboard"
    },
    "maintainer": {
        "name": "Julien Malik",
        "email": "julien.malik@paraiso.me"
    },
    "multi_instance": "false",
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "ask": {
                    "en": "Choose a domain for ynhstatus"
                },
                "example": "example.com",
                "default": "ynh-status.paraiso.me"
            },
            {
                "name": "path",
                "ask": {
                    "en": "Choose a path for ynhstatus"
                },
                "example": "/",
                "default": "/"
            }
        ]
    }
}
